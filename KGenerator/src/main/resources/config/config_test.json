{
  "params": {
    "package": "com.kermit",
    "author": "Kermit Sun"
  },
  "jdbc": {
    "driver": "com.mysql.jdbc.Driver",
    "url": "jdbc:mysql://127.0.0.1:3306/test",
    "username": "dev",
    "password": "dev",
    "catalog": null,
    "schemaPattern": "test",
    "tableNamePattern": null,
    "types": ["TABLE"],
    "tables": ["t_test1","t_test2"]
  },
  "export": {
    "outPath": "E:/KGenerator/",
    "clearDirs": true,
    "items": [
      {
        "outPath": "${tableName}/dao/",
        "templateName": "dao.ft",
        "fileName": "${tableName}Dao.java"
      },{
        "outPath": "${tableName}/dao/impl/",
        "templateName": "daoImpl.ft",
        "fileName": "${tableName}DaoImpl.java"
      },{
        "outPath": "${tableName}/service/",
        "templateName": "service.ft",
        "fileName": "${tableName}Service.java"
      },{
        "outPath": "${tableName}/service/impl/",
        "templateName": "serviceImpl.ft",
        "fileName": "${tableName}ServiceImpl.java"
      },{
        "outPath": "${tableName}/mapper/",
        "templateName": "mapper.ft",
        "fileName": "${tableName}Mapper.xml"
      },{
        "outPath": "${tableName}/model/",
        "templateName": "model.ft",
        "fileName": "${tableName}Model.java"
      }
    ]
  }
}